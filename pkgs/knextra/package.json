{
  "type": "module",
  "name": "knextra",
  "version": "0.0.0-alpha.0",
  "description": "Thin wrapper around Knex",
  "author": "Slee Woo",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": ["pkg/*"],
  "exports": {
    ".": {
      "types": "./pkg/index.d.ts",
      "default": "./pkg/index.js"
    }
  },
  "scripts": {
    "build": "esbuilder src/index.ts -o pkg -s @/scripts/"
  },
  "peerDependencies": {
    "knex": "^3.1.0"
  }
}
