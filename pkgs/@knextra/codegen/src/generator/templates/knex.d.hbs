declare module "knex/types/tables" {
  interface Tables {
  {{#each tables}}
    "{{moduleName}}": import("knex").Knex.CompositeTableType<
      import("./{{name}}/types").RecordT,
      import("./{{name}}/types").InsertT,
      import("./{{name}}/types").UpdateT,
      import("./{{name}}/types").QueryT
    >;
  {{/each}}
  {{#each views}}
    "{{moduleName}}": import("knex").Knex.CompositeTableType<
      import("./{{name}}/types").RecordT
    >;
  {{/each}}
  }
}

// needed for declared modules to be treated as augmented rather than ambient
export type {}
