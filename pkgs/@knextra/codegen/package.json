{
  "type": "module",
  "name": "@knextra/codegen",
  "version": "0.0.0-alpha.0",
  "author": "Slee Woo",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": ["pkg/*"],
  "bin": {
    "knextra-codegen": "pkg/cli.js"
  },
  "exports": {
    ".": {
      "types": "./pkg/index.d.ts",
      "default": "./pkg/index.js"
    }
  },
  "scripts": {
    "build": "esbuilder src/index.ts src/cli.ts -o pkg -l .hbs=text -s @/scripts/",
    "postbuild": "chmod +x pkg/cli.js"
  },
  "dependencies": {
    "@knextra/stdlib": "workspace:^",
    "crc": "^4.3.2",
    "extract-pg-schema": "^5.3.5",
    "fs-extra": "^11.3.0",
    "kleur": "^4.1.5",
    "lodash-es": "^4.17.21",
    "pluralize": "^8.0.0",
    "smol-toml": "^1.3.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/pluralize": "^0.0.33",
    "knex": "^3.1.0"
  }
}
